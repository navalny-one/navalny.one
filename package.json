{
    "name": "navalny.one",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "prettier": "prettier --write **/*.{html,json,js,ts,tsx,css,scss}",
        "lint-staged": "lint-staged",
        "prepare": "is-ci || husky install"
    },
    "lint-staged": {
        "*.{html,json,js,ts,tsx,css,scss}": [
            "prettier --write"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@types/node": "14.14.37",
        "@types/react": "17.0.3",
        "@types/react-dom": "17.0.3",
        "@typescript-eslint/eslint-plugin": "4.20.0",
        "@typescript-eslint/parser": "4.20.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-prettier": "3.3.1",
        "husky": "6.0.0",
        "is-ci": "3.0.0",
        "lint-staged": "10.5.4",
        "prettier": "2.2.1",
        "react-scripts": "4.0.3",
        "sass": "1.32.8",
        "typescript": "4.2.3"
    }
}
